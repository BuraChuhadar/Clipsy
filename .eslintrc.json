{
    "parserOptions": {
        "ecmaVersion": 6,
        "sourceType": "module",
        "ecmaFeatures": {
            "jsx": true
        }
    },
    "env": {
        "jest": true,
        "browser": true
    },
    "extends": ["eslint:recommended", "plugin:react/recommended"],
    "rules": {
        "semi": 2,
        "react/prop-types": 2,
        "react/sort-prop-types": 2
    }
}
